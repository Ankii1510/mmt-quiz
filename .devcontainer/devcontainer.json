{
  "name": "MMT Quiz Codespace",
  "image": "mcr.microsoft.com/vscode/devcontainers/javascript-node:0-20",
  "forwardPorts": [
    3000
  ],
  "postCreateCommand": "npm install -g http-server",
  "customizations": {
    "vscode": {
      "extensions": [
        "ritwickdey.LiveServer",
        "esbenp.prettier-vscode"
      ]
    }
  },
  "remoteUser": "vscode"
}